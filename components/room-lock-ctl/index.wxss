.panel{
  margin: 0 20rpx 20rpx;
  background-color: #fff;
  box-shadow: 0px 1px 4px 2px rgba(0,0,0,0.04);
  border-radius: 15rpx;
  position: relative;
}
.roombgname{
  position: absolute;
  z-index: 1;
  bottom: 0;
  left: 0;font-size: 42px;
  color: rgba(0,0,0,0.1);
}
.lock-control {
  overflow: hidden;
  position: relative;
  margin: 20rpx;
  background-color: #fff;
  box-shadow: 0px 1px 4px 2px rgba(0,0,0,0.04);
  border-radius: 15rpx;
}
.lock-control .open {
  margin: 0 32rpx 33rpx;
  border: 1px solid rgba(30,94,247,0.6);
  border-radius: 8rpx;
  padding: 41rpx 32rpx;
  background: rgba(30,94,247,0.08);
  display: flex;
  justify-content: space-between;
  align-items: center;
}


.ble {
  height: 90rpx;
  margin: 0;
  padding: 0 20rpx;
  line-height: 40rpx;
  color: rgba(0,0,0,.9);
  font-size: 36rpx;
  font-weight: 500;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ble .tit {
  font-size: 32rpx;
  font-weight: 500;
  flex: 1;
}
.ble .choose{
  width: 40rpx;
}
.ble .choose .img{
  height: 40rpx;
  width: 40rpx;
}
.ble .volume{
  width: 60rpx;
  margin-left: 10px;
}
.ble .volume .img{
  width: 40rpx;
  height: 40rpx;
}

.lock-control .tip {
  color: rgba(0,0,0,.8);
  font-size: 28rpx;
  font-weight: 500;
  margin-bottom: 10rpx;
}

.lock-control .sub-tip {
  font-size: 24rpx;
  color: rgba(0,0,0,0.6);
  display: flex;
  align-items: center;
}
.sub-tip .icon-license{
  color: #4ABA99;
  font-size: 32rpx;
}

.ble_tit{
  padding: 18rpx 20rpx;
  font-size: 14px;
  color: #333;
}
.ble_tit text{
  font-size: 12px;
}

.lock-control .btn {
  text-align: center;
  width: 180rpx;
  height: 72rpx;
  line-height: 72rpx;
  background: linear-gradient(270deg,rgba(55,97,255,1) 0%,rgba(68,157,255,1) 100%);
  border-radius: 36rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #fff;
}
.lock-control .operate {
  display: flex;
  height: 100rpx;
  border-top: 1px solid rgba(237, 237, 237, 1);
}

.lock-control .operate > view {
  position: relative;
  flex: 1;
  text-align: center;
  font-size: 24rpx;
  color: rgba(0,0,0,.6);
  display: flex;
  justify-content: center;
  align-items: center;
}
.lock-control .operate image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 12rpx;
}
.icon-naozhong{
  margin-right: 10rpx;
  font-size: 36rpx;
}
.lock-control .operate > view:not(:last-of-type)::after {
  position: absolute;
  content: '';
  top: 0;
  right: 0;
  bottom: 0;
  height: 100%;
  width: 1px;
  background: rgba(237, 237, 237, 1);
}
.blue_msg{
  display: flex;
  align-items: center;
  color: #ED9B17;
  padding: 5px;
}
.blue_msg .icon-warning {
  /*color: #4ABA99;*/
  font-size: 18px;
}
.blue_msg{
  font-size: 12px;
}
.password{
  height: 144rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background:rgba(246,247,249,1);
  border-radius:6px;
  padding: 0 11rpx;
}
.time-char{
  width: calc((100% - 132rpx) / 8);
  border-radius:3px;
  border:1px solid rgba(0,0,0,0.1);
  text-align: center;
  line-height: 84rpx;
  font-size: 24px;
  font-weight: 600;
  height: 84rpx;
  background: #fff;
}
.ctn{
  overflow: hidden;
}
.question_desc{
  font-size: 12px;
  position: absolute;top:10px;
  right: 10px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.question{
  width: 20px;
  height: 20px;
}
.border-top{
  border-top: 1px solid rgba(237, 237, 237, 1);
}
.relative{
  position: relative;
  min-height: 112px;
}
.open.http{
  position: absolute;
  top: 0;left: 0;
  width: calc(100% - 128rpx);
  background: aliceblue;
  border: 1px solid rgba(0,0,0,.6);
}
.android_img{
  width: 100%;
  height: 400px;
}
.ios_img{
  width: 100%;
  height: 200px;
}